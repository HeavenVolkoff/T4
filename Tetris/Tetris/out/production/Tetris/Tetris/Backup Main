        //Material Def
        mat = new Material(assetManager, "Common/MatDefs/Misc/ShowNormals.j3md");

        //Set RootNode to Define Getter
        mainRootNode = rootNode;

        //Normal
        genPiece(rootNode, mat);

        //Normal
        genPiece(mainRootNode, mat);

        //Light
        DirectionalLight sun = new DirectionalLight();
        sun.setDirection(new Vector3f(0,1,-5).normalizeLocal());
        sun.setColor(ColorRGBA.White);
        rootNode.addLight(sun);

        //Fixed Cam
		flyCam.setEnabled(false);
